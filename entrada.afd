// Alfabeto das máquinas (somente 1 símbolo).
A -> {0, 1};

// Descrição da máquina 1 cujo nome é M1.
M1 {
	E -> {A, B};  // Estados (1 ou mais símbolos).
	T -> { (A, 0) = B, (B, 0) = B, (B, 1) = B }; // Transições.
	i -> A; // Estado inicial.
	F -> {B}; //Estados finais.
}

// Descrĩção da máquina 2 cujo nome é M2.
M2 {
	E -> {A, B};  // Estados (1 ou mais símbolos).
	T -> { (A, 0) = A, (A, 1) = B, (B, 0) = A, (B, 1) = B }; // Transições.
	i -> A; // Estado inicial.
	F -> {B}; //Estados finais.	
}


// Descrĩção da máquina 2 cujo nome é M2.
M3 {
	E -> {A};  // Estados (1 ou mais símbolos).
	T -> {}; // Transições.
	i -> A; // Estado inicial.
	F -> {A}; //Estados finais.	
}